<h1><%= @registered_application.name %></h1>

<div class="row">

  <div class="col-md-8">
    <h3> <%= @registered_application.url %>
    <%= link_to "Remove Registration", @registered_application, method: :delete, class: 'btn btn-danger', data: {confirm: 'Are you sure you want to remove this app from registration?'} %> </h3>

    <% @events.each do | event, count | %>
      <p> <%=h "#{event}.........count  #{count.length}" %> </p>
    <% end %>
  </div>

</div>
